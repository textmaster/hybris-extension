<?xml version="1.0" encoding="ISO-8859-1"?>

<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="items.xsd">

    <enumtypes>
        <enumtype generate="true" code="TextMasterProjectStatusEnum" autocreate="true" dynamic="true">
            <value code="IN_CREATION"/>
            <value code="IN_PROGRESS"/>
            <value code="IN_REVIEW"/>
            <value code="COMPLETED"/>
            <value code="PAUSED"/>
            <value code="CANCELED"/>
        </enumtype>

        <enumtype generate="true" code="TextMasterDocumentStatusEnum" autocreate="true" dynamic="true">
            <value code="IN_CREATION"/>
            <value code="WAITING_ASSIGNMENT"/>
            <value code="IN_PROGRESS"/>
            <value code="IN_REVIEW"/>
            <value code="COMPLETED"/>
            <value code="INCOMPLETE"/>
            <value code="PAUSED"/>
            <value code="CANCELED"/>
            <value code="COPYSCAPE"/>
            <value code="COUNTING_WORDS"/>
            <value code="QUALITY_CONTROL"/>
        </enumtype>
    </enumtypes>

    <relations>

        <relation code="TextMasterAccount2Projects" localized="false">
            <description>Relation between account and projects</description>
            <sourceElement type="TextMasterAccount" qualifier="account" cardinality="one" navigable="true"/>
            <targetElement type="TextMasterProject" qualifier="projects" cardinality="many" navigable="true"/>
        </relation>

        <relation code="TextMasterProjectToDocuments" localized="false">
            <description>Relation between project and items</description>
            <sourceElement type="TextMasterProject" qualifier="project" cardinality="one" navigable="true"/>
            <targetElement type="TextMasterDocument" qualifier="documents" cardinality="many" navigable="true">
                <modifiers partof="true"/>
            </targetElement>
        </relation>

        <relation code="TextMasterProjectToAttributes" localized="false">
            <description>Relation between project and attributes</description>
            <deployment table="tmProjects2Attributes" typecode="15011"/>
            <sourceElement type="TextMasterProject" cardinality="many" navigable="false"/>
            <targetElement type="AttributeDescriptor" qualifier="attributes" cardinality="many" navigable="true"/>
        </relation>

        <relation code="TextMasterDocumentToDocAttributes" localized="false">
            <description>Relation between document and attributes</description>
            <sourceElement type="TextMasterDocument" qualifier="document" cardinality="one" navigable="true"/>
            <targetElement type="TextMasterDocumentAttribute" qualifier="documentAttributes" cardinality="many" navigable="true">
                <modifiers partof="true"/>
            </targetElement>
        </relation>

        <relation code="TextMasterDocumentToMessages" localized="false">
            <description>Relation between document and messages</description>
            <sourceElement type="TextMasterDocument" qualifier="document" cardinality="one" navigable="true"/>
            <targetElement type="TextMasterSupportMessage" qualifier="supportMessages" cardinality="many" navigable="true">
                <modifiers partof="true"/>
            </targetElement>
        </relation>

        <relation code="TextMasterConfigurationToItemType" localized="false">
            <description>Relation between configuration and item types</description>
            <sourceElement type="TextMasterConfiguration" qualifier="configuration" cardinality="one" navigable="true"/>
            <targetElement type="TextMasterItemType" qualifier="translatableList" cardinality="many" navigable="true">
                <modifiers partof="true"/>
            </targetElement>
        </relation>

        <relation code="TextMasterItemTypeToAttributeDescriptor" localized="false">
            <description>Relation between item type and attributes</description>
            <deployment table="tmItemType2Attributes" typecode="15012"/>
            <sourceElement type="TextMasterItemType" qualifier="textMasterItemType" cardinality="many" navigable="true"/>
            <targetElement type="AttributeDescriptor" qualifier="preselectedAttributes" cardinality="many" navigable="true" />
        </relation>

    </relations>

    <itemtypes>

        <itemtype code="TextMasterAccount">
            <deployment table="textmasteraccount" typecode="15001"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>Autogenerated code of account</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="name" type="java.lang.String">
                    <description>Name of account</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="apiKey" type="java.lang.String">
                    <description>The api key to connect to TextMaster</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="apiSecret" type="java.lang.String">
                    <description>The api secret key to connect to TextMaster</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="configuration" type="TextMasterConfiguration">
					<description>Configuration of the account</description>
					<persistence type="property"/>
				</attribute>
            </attributes>
        </itemtype>


        <itemtype code="TextMasterProject">
            <deployment table="textmasterproject" typecode="15002"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>Autogenerated code of project</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="name" type="java.lang.String">
                    <description>Name of project</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="remoteId" type="java.lang.String">
                    <description>Project ID assigned by TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="reference" type="java.lang.String">
                    <description>Reference code assigned by TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="templateId" type="java.lang.String">
                    <description>Template ID supplied by TextMaster</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="translatedItemType" type="ComposedType">
                    <description>Type of objects to translate</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="languageSource" type="TextMasterLanguage">
                    <description>Source of language of translations</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="languageTarget" type="TextMasterLanguage">
                    <description>Target of language of translations</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="status" type="TextMasterProjectStatusEnum">
                    <description>Status of project defined in TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="totalWordCount" type="java.lang.Integer">
                    <description>Total word count of project</description>
                    <defaultvalue>Integer.valueOf(0)</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="price" type="java.math.BigDecimal">
                    <description>Total amount of the project</description>
                    <defaultvalue>java.math.BigDecimal.valueOf(0)</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="currencyIsocode" type="java.lang.String">
                    <description>The ISO code of the currency associated to the total amount</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="autolaunch" type="java.lang.Boolean">
                    <description>Define if the autolaunch mode is activated</description>
                    <modifiers optional="false"/>
                    <defaultvalue>Boolean.FALSE</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="finalized" type="java.lang.Boolean">
                    <description>Define if the finalize process has been executed</description>
                    <modifiers optional="false"/>
                    <defaultvalue>Boolean.FALSE</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="translationMemoryActivated" type="java.lang.Boolean">
                    <description>Define if the translation memory option is activated on template</description>
                    <modifiers optional="false"/>
                    <defaultvalue>Boolean.FALSE</defaultvalue>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="TextMasterDocument">
            <deployment table="textmasterdocument" typecode="15003"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>Autogenerated code of document</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="remoteId" type="java.lang.String">
                    <description>Document ID assigned by TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="reference" type="java.lang.String">
                    <description>Reference code assigned by TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="item" type="Item">
                    <description>Hybris item which the document is attached</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="status" type="TextMasterDocumentStatusEnum">
                    <description>Status of document defined in TextMaster platform</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="wordCount" type="java.lang.Integer">
                    <description>Word count of document</description>
                    <defaultvalue>Integer.valueOf(0)</defaultvalue>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="TextMasterDocumentAttribute">
            <deployment table="textmasterdocattribute" typecode="15004"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>Autogenerated code of document attribute</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="originalContent" type="java.lang.String">
                    <description>Original content of item pushed on TextMaster platform</description>
                    <persistence type="property">
                        <columntype database="oracle">
                            <value>CLOB</value>
                        </columntype>
                        <columntype database="sap">
                            <value>NCLOB</value>
                        </columntype>
                        <columntype>
                            <value>HYBRIS.LONG_STRING</value>
                        </columntype>
                    </persistence>
                </attribute>
                <attribute qualifier="translatedContent" type="java.lang.String">
                    <description>Translated content proposed by author from TextMaster platform</description>
                    <persistence type="property">
                        <columntype database="oracle">
                            <value>CLOB</value>
                        </columntype>
                        <columntype database="sap">
                            <value>NCLOB</value>
                        </columntype>
                        <columntype>
                            <value>HYBRIS.LONG_STRING</value>
                        </columntype>
                    </persistence>
                </attribute>
                <attribute qualifier="attribute" type="AttributeDescriptor">
                    <description>Attribute attached to the document</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="TextMasterLanguage">
            <deployment table="textmasterlanguage" typecode="15005"/>
            <attributes>
                <attribute qualifier="isocode" type="java.lang.String">
                    <description>Remote ISO code of the language</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="active" type="java.lang.Boolean">
                    <description>Define if the current language is active</description>
                    <modifiers optional="false"/>
                    <defaultvalue>Boolean.TRUE</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="language" type="Language">
                    <description>The hybris language associated</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="TextMasterSupportMessage">
            <deployment table="textmastersupportmessage" typecode="15006"/>
            <attributes>
                <attribute qualifier="code" type="java.lang.String">
                    <description>Autogenerated code of document</description>
                    <modifiers optional="false"/>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="message" type="java.lang.String">
                    <description>Support message</description>
                    <persistence type="property">
                        <columntype database="oracle">
                            <value>CLOB</value>
                        </columntype>
                        <columntype database="sap">
                            <value>NCLOB</value>
                        </columntype>
                        <columntype>
                            <value>HYBRIS.LONG_STRING</value>
                        </columntype>
                    </persistence>
                </attribute>
                <attribute qualifier="authorId" type="java.lang.String">
                    <description>Author TextMaster ID</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="authorReference" type="java.lang.String">
                    <description>Author TextMaster Reference</description>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="hybrisOwner" type="java.lang.Boolean">
                    <description>Define if the hybris user is at the origin of the message</description>
                    <modifiers optional="false"/>
                    <defaultvalue>Boolean.TRUE</defaultvalue>
                    <persistence type="property"/>
                </attribute>
                <attribute qualifier="date" type="java.util.Date">
                    <description>The date of the message</description>
                    <persistence type="property"/>
                </attribute>
            </attributes>
        </itemtype>

        <itemtype code="TextMasterProjectTask" extends="Task">
            <description>Represent a task for project process.</description>
            <attributes>
                <attribute qualifier="textMasterProject" type="TextMasterProject">
                    <persistence type="property"/>
                    <description>The project related</description>
                </attribute>
            </attributes>
        </itemtype>
        
		<itemtype code="TextMasterItemType">
			<deployment table="textmasteritemtype" typecode="15007"/>
			<attributes>
				<attribute qualifier="code" type="java.lang.String">
					<description>Autogenerated code of the TextMaster item type</description>
					<modifiers optional="false"/>
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="translatedItemType" type="ComposedType">
					<description>The type of the item</description>
					<modifiers optional="false"/>
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>
		
		<itemtype code="TextMasterConfiguration">
			<deployment table="textmasterconfiguration" typecode="15008"/>
			<attributes>
				<attribute qualifier="code" type="java.lang.String">
					<description>Autogenerated code of the configuration</description>
					<modifiers optional="false"/>
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="defaultSourceLanguage" type="Language">
					<description>Default Source Language</description>
					<persistence type="property"/>
				</attribute>
				<attribute qualifier="defaultTargetLanguage" type="Language">
					<description>Default Target Language</description>
					<persistence type="property"/>
				</attribute>
			</attributes>
		</itemtype>
		
    </itemtypes>

</items>
